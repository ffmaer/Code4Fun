	public static int base(String input) {
		// input: 11001001
		HashMap<String, Integer> map = new HashMap<String, Integer>();

		for (int i = 0; i < input.length(); i++) {
			if (!map.containsKey(input.charAt(i))) {
				//char to String
				map.put("" + input.charAt(i), new Integer(1));
			}
		}
		return map.size();
	}
	
